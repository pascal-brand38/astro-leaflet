  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pascal-brand38/astro-leaflet.git"
  },
  "author": "Pascal Brand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pascal-brand38/astro-leaflet/issues"
  },
